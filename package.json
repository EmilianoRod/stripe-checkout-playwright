{
  "name": "stripe-checkout-playwright",
  "version": "1.0.0",
  "private": true,
  "description": "Playwright tests for Stripe Checkout",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "playwright test",
    "test:chrome": "playwright test --project=chrome",
    "report": "playwright show-report",
    "show-trace": "playwright show-trace",
    "postinstall": "playwright install --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.1"
  },
  "dependencies": {
    "stripe": "^18.5.0"
  }
}
